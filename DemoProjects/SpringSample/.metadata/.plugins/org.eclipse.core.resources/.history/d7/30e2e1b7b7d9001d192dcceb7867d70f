import java.util.Arrays;

public class HashMapClass {

	public static void main(String[] args) {

		int[] arra = { 10, 20, 30, 40, 50, 60 };

	       int a = arra[arra.length-1], i;
	       for (i = arra.length-1; i > 0; i--)
	          arra[i] = arra[i-1];
	       arra[0] = a;
	       
	       
		System.out.println(Arrays.toString(arra));

	}

}
