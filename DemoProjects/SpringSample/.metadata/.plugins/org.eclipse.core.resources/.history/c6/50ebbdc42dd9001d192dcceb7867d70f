public class HashMapClass {

	public static void main(String[] args) {

		int[] arr = { 0, 1, 0, 1, 1, 0 };
		int left_pointer = 0;
		int right_pointer = arr.length - 1;
		int length = arr.length;

		for (int i = 0; i < arr.length; i++) {
			if(arr[i]>arr[length-i]) {
				arr[i] = 0;
				arr[length-i] = 1;
				
			}
		}

	}

}
