import java.util.Arrays;

public class HashMapClass {

	public static void main(String[] args) {

		int[] arr = { 20, 70, 40, 10, 60, 30 };

		for (int i = 0; i < arr.length; i++) {
			
			int val = sum(i+1,arr);
			
		}

	}

	private static int sum(int i, int[] arr) {
		int sum=0;
		for(int j=i;j<arr.length;j++) {
			sum = sum+arr[j];
		}

		return sum;
		
	}

}
